#include <stdio.h>

int main()
{
    int limit,i,values[100],sum;
    printf("enter the limit");
    scanf("%d",&limit);
    printf("enter the values");
for(i=0;i<limit;i++)
{
    scanf("%d",&values[i]);
}
for(i=0;i<limit;i++)
{
    sum=sum +values[i];
}
printf("%d",sum);
    return 0;
}
