#include <stdio.h>

int
main ()
{
    int a[100];
    int i,limit;
    printf("enter the array limit");
    scanf("%d",&limit);
    printf("enter the value");
  for(i=0;i<limit;i++)
  {
     scanf("%d",&a[i]); 
  }
  printf("entered values are:\t");
for(i=0;i<limit;i++)
{
    printf("%d\t",a[i]);
}
  return 0;
}
