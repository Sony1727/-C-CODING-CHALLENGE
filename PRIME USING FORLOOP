#include <stdio.h>

int main() {
    int n,flag=0,i;
    printf("ENTER THE NUMBER:");
    scanf("%d",&n);
    for(i=2;i<n;i++){
        if(n%i==0){
            flag=1;
            break;
        }
    }
if(flag==0){
    printf("it is a prime number");
}else{
    printf("it is not prime number");
}
    return 0;
}
