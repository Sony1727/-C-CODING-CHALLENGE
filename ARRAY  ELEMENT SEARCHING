#include <stdio.h>

int main()
{
    int limit,values[100],i,searchkey,flag=0;
printf("enter the limit:");
scanf("%d",&limit);

printf("enter the values:");
for(i=0;i<limit;i++){
    scanf("%d",&values[i]);
}

printf("please  enter searchkey:");
scanf("%d",&searchkey);
for(i=0;i<limit;i++){
   if(searchkey==values[i]){
   flag=1; 
break; 
}

}

if(flag==1){
    printf("value at position %d",i+1);
}
else
{
    printf("values not found");
}
    return 0;
}
